{
  "name": "debug-snippets",
  "description": "VS Code Debug snippets for JS, TS and CSS",
  "version": "1.7.0",
  "displayName": "Debug Snippets",
  "publisher": "deinsoftware",
  "icon": "images/light-icon.png",
  "keywords": [
    "debugger",
    "console",
    "log",
    "snippets",
    "css",
    "javascript",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deinsoftware/vscode-debug-snippets"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/deinsoftware"
  },
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Snippets"
  ],
  "main": "./build/extension.js",
  "browser": "./build/extension.js",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "activationEvents": [
    "onCommand:extension.snippetSearch"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/code.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/context.json"
      },
      {
        "language": "jsx-attr",
        "path": "./snippets/code.json"
      },
      {
        "language": "jsx-attr",
        "path": "./snippets/context.json"
      },
      {
        "language": "jsx-attr",
        "path": "./snippets/react.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/code.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/context.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/context.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/react.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/code.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/context.json"
      },
      {
        "language": "tsx-attr",
        "path": "./snippets/code.json"
      },
      {
        "language": "tsx-attr",
        "path": "./snippets/context.json"
      },
      {
        "language": "tsx-attr",
        "path": "./snippets/react.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/code.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/context.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/react.json"
      },
      {
        "language": "vue",
        "path": "./snippets/code.json"
      },
      {
        "language": "vue",
        "path": "./snippets/context.json"
      },
      {
        "language": "css",
        "path": "./snippets/styles.json"
      },
      {
        "language": "scss",
        "path": "./snippets/styles.json"
      }
    ]
  },
  "scripts": {
    "test": "vitest --run --reporter verbose",
    "test:w": "vitest",
    "test:ui": "vitest --ui"
  },
  "volta": {
    "node": "v22.11.0",
    "npm": "10.9.0"
  },
  "devDependencies": {
    "vitest": "3.0.7"
  }
}
